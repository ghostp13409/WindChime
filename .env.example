# WindChime App Environment Variables Template
# Copy this file to .env and fill in your actual values

# Feedback Form Configuration
FORMSPREE_ENDPOINT=your_formspree_endpoint_here

# API Keys (add as needed)
# GOOGLE_MAPS_API_KEY=your_google_maps_key_here
# FIREBASE_API_KEY=your_firebase_key_here
# ANALYTICS_KEY=your_analytics_key_here

# Database Configuration (if using remote databases)
# DATABASE_URL=your_database_url_here
# DATABASE_PASSWORD=your_database_password_here

# Third-party Service Keys
# STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
# STRIPE_SECRET_KEY=your_stripe_secret_key_here
# PAYPAL_CLIENT_ID=your_paypal_client_id_here

# Push Notification Services
# FCM_SERVER_KEY=your_fcm_server_key_here
# APNS_KEY_ID=your_apns_key_id_here

# Social Media Integration
# FACEBOOK_APP_ID=your_facebook_app_id_here
# TWITTER_API_KEY=your_twitter_api_key_here

# Development/Debug Settings
DEBUG_MODE=true
LOG_LEVEL=info

# App Store Configuration
# PLAY_STORE_UPLOAD_KEY=your_play_store_key_here
# APP_STORE_CONNECT_KEY=your_app_store_key_here